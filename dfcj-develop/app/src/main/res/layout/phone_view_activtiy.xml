<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    >

    <data>

    </data>


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        >

        <include android:id="@+id/title_top" layout="@layout/unified_head2"></include>



        <FrameLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">


            <ProgressBar
                android:id="@+id/phone_view_load"
                android:layout_width="30dp"
                android:layout_height="30dp"
                style="@style/mProgress_circle"
                android:layout_gravity="center"
                />

            <com.luck.picture.lib.photoview.PhotoView
                android:id="@+id/phone_view"
                android:layout_width="match_parent"
                android:layout_height="match_parent"></com.luck.picture.lib.photoview.PhotoView>



        </FrameLayout>


    </LinearLayout>


</layout>